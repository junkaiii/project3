<div class="main-window">

    <div class="full-search">

        <!-- Advanced Search Function -->
        <div class="adv-search-engine">
          By
          <select id="travel_method">
              <option value="walking">Walking</option>
              <option value="driving">Driving</option>
          </select>
           for
            <input id="travel_time" type="number" min="1" max="99" placeholder="1 - 99"> minutes from - -
        </div>


        <!-- Normal Search Function -->
        <div class="search-engine">
            <div class="search bar">
                <input type="search" id="search_bar" placeholder="Add your address here">
                <button type="submit" id="locationbtn" class="btn btn-warning btn-md btn-font">Search</button>
            </div>
            <div class="or">
                or
            </div>
            <div class="autolocatordiv">
                <button type="submit" id="autolocator" class="btn btn-warning btn-md btn-font">Use my location</button>
            </div>
        </div>
        <div class="sentence-end">
          - - where can I eat?
        </div>
    </div>


    <!-- Google Map Window -->
    <div class="gmap-window">
        <div id="map_canvas"></div>
    </div>
</div>
